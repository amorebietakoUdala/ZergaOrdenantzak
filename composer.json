{
    "name": "amorebietakoudala/zergaordenantzak",
    "license": "MIT",
    "type": "project",
    "description": "Zerga ordenantzen aplikazioa",
    "autoload": {
        "psr-4": { 
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Tests\\App\\": "tests/Bundle",
            "App\\Tests\\": "tests/" 
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "amorebietakoudala/stimulus-controller-bundle": "^1.0",
        "amorebietakoudala/user-bundle": "^2.0",
        "babdev/pagerfanta-bundle": "^3.8",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.5",
        "friendsofsymfony/ckeditor-bundle": "^2.5",
        "friendsofsymfony/jsrouting-bundle": "^3.5",
        "friendsofsymfony/rest-bundle": "^3.6",
        "guzzlehttp/guzzle": "^6.3",
        "jms/serializer-bundle": "^5.0",
        "knplabs/knp-menu-bundle": "*",
        "nelmio/api-doc-bundle": "^4.23",
        "nelmio/cors-bundle": "*",
        "pagerfanta/twig": "^4.7",
        "phpoffice/phpword": "^0.12.1",
        "qipsius/tcpdf-bundle": "^2.0",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "6.4.*",
        "symfony/console": "^6.0",
        "symfony/flex": "^1.21",
        "symfony/form": "6.4.*",
        "symfony/framework-bundle": "6.4.*",
        "symfony/ldap": "6.4.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/polyfill-php72": "^1.10",
        "symfony/translation": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/validator": "6.4.*",
        "symfony/webpack-encore-bundle": "^1.17",
        "symfony/yaml": "6.4.*",
        "tecnickcom/tcpdf": "^6.8.0",
        "willdurand/js-translation-bundle": "^6.0"
    },
    "require-dev": {
        "rector/rector": "^1.2",
        "symfony/debug-bundle": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/maker-bundle": "^1.43",
        "symfony/phpunit-bridge": "6.4.*",
        "symfony/stopwatch": "6.4.*",
        "symfony/web-profiler-bundle": "6.4.*"
    },
    "config": {
        "platform": {
            "php": "8.2.27"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },    
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --tag=4.21.0 --clear=drop": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    }
}
