{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-4": { 
            "App\\": "src/",
            "UserBundle\\": "src/UserBundle"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Tests\\App\\": "tests/Bundle",
            "App\\Tests\\": "tests/" 
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.11.1",
                "dist": {
                    "url": "https://code.jquery.com/jquery-1.11.1.js",
                    "type": "file"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "amorebietakoudala/user-bundle": "^1.4.6",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "doctrine/orm": "^2.5",
        "fortawesome/font-awesome": "~4.0",
        "friendsofsymfony/ckeditor-bundle": "2.0",
        "friendsofsymfony/jsrouting-bundle": "^2.8",
        "friendsofsymfony/rest-bundle": "^2.4",
        "guzzlehttp/guzzle": "^6.3",
        "jms/serializer-bundle": "^2.1",
        "jquery/jquery": "1.11.*",
        "knplabs/knp-menu-bundle": "^2.0",
        "mbence/opentbs-bundle": "^1.9",
        "nelmio/api-doc-bundle": "2.13.5",
        "nelmio/cors-bundle": "^1.4",
        "phpoffice/phpword": "^0.12.1",
        "sensio/framework-extra-bundle": "^6.2",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.4.*",
        "symfony/console": "*",
        "symfony/flex": "^1.21",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "*",
        "symfony/ldap": "4.4",
        "symfony/maker-bundle": "^1.39",
        "symfony/monolog-bundle": "^3.6",
        "symfony/polyfill-php72": "^1.10",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.17",
        "symfony/yaml": "*",
        "tecnickcom/tcpdf": "6.2.17",
        "twbs/bootstrap": "3.0.*",
        "twig/extensions": "^1.5",
        "white-october/pagerfanta-bundle": "^1.0",
        "whiteoctober/tcpdf-bundle": "^1.0",
        "willdurand/js-translation-bundle": "^4.0"
    },
    "require-dev": {
        "rector/rector": "^1.2",
        "symfony/debug-bundle": "4.4.*",
        "symfony/dotenv": "*",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/stopwatch": "4.4.*",
        "symfony/web-profiler-bundle": "4.4.*"
    },
    "config": {
        "platform": {
            "php": "7.2.36"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },    
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
