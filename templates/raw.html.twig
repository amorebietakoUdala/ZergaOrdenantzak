        {{ encore_entry_link_tags('app') }}
    {% endblock %}
</head>

<body data-spy="scroll" data-target="#navbar-example">
<div id="navbar-example">
   {% block menua %}{% endblock %}
</div>

<div class="container">
    {% block body %}{% endblock %}
</div> <!-- /container -->

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</body>
</html>